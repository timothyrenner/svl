DATASETS 
    bigfoot "data/bigfoot_sightings.csv"

LINE bigfoot
    X date BY YEAR
    Y COUNT date
    SPLIT BY classification

CONCAT(
    BAR bigfoot
        X classification
        Y COUNT temperature_mid
    
    (
        HISTOGRAM bigfoot
            temperature_high
            BINS 25
    
        HISTOGRAM bigfoot
            temperature_mid
            BINS 25
        
        HISTOGRAM bigfoot
            temperature_low
            BINS 25
    )
)

SCATTER bigfoot
    X latitude
    Y moon_phase