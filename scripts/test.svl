DATASETS 
    bigfoot "data/bigfoot_sightings.csv"

LINE bigfoot
    TITLE "Bigfoot Sightings by Year"
    X date BY YEAR LABEL "Year of Sighting"
    Y COUNT date LABEL "Number of Sightings"
    SPLIT BY classification

CONCAT(
    BAR bigfoot
        TITLE "Bigfoot Sightings by Classification"
        X classification LABEL "Classification"
        Y COUNT temperature_mid LABEL "Number of Sightings"
    
    (
        CONCAT(
            HISTOGRAM bigfoot
                TITLE "Bigfoot Sighting High Temperature"
                temperature_high
                BINS 25
                LABEL "Temperature (F)"

            HISTOGRAM bigfoot
                TITLE "Bigfoot Sighting Low Temperature"
                temperature_low
                BINS 25
                LABEL "Temperature (F)"
        )
    
        HISTOGRAM bigfoot
            TITLE "Bigfoot Sighting Mid Temperature"
            temperature_mid
            BINS 25
            LABEL "Temperature (F)"
        

    )
)

SCATTER bigfoot
    TITLE "Bigfoot Sighting Moon Phase by Latitude"
    X latitude LABEL "Latitude (deg)"
    Y moon_phase LABEL "Moon Phase"